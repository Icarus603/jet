### A 2D point structure.
pub struct Point:
    x: float
    y: float

### Calculates the distance between two points.
pub fn distance(p1: Point, p2: Point) -> float:
    let dx = p2.x - p1.x
    let dy = p2.y - p1.y
    sqrt(dx * dx + dy * dy)
